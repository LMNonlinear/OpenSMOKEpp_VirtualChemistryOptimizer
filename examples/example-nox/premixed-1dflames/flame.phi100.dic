Dictionary PremixedLaminarFlame1D
{
	@Type					FlameSpeed;
	@KineticsFolder			../../../Mechanisms/virtual-chemistry-NO/2018.06.19/mechanism/kinetics;
	
	@InletStream 	inlet-stream;
	@OutletStream	outlet-stream;
	
	@InletVelocity	40 cm/s;
	
	@Soret			false;
	
	@Grid			grid;
	
	@Output			Output;
	
	@UseNlsSolver	false;

	@Optimization		optimization;
	
	@Backup        		premixed-1dflames/flame.phi100.backup.xml;
}

Dictionary inlet-stream
{
	@MassFractions	FUEL 0.055045872
					OX   0.220183486
					N2   0.724770642;
						
	@Temperature	300 K;
	@Pressure		1 atm;
}

Dictionary outlet-stream
{
	@MassFractions	I    0.28
					N2   0.72;

	@Temperature	2100 K;
	@Pressure		1 atm;
}

Dictionary grid
{
	@Length				100 cm;
	@InitialPoints		12;
	@Type				database;
	
	@MaxPoints				700;
	@MaxAdaptivePoints		15;
	@GradientCoefficient	0.0075;
	@CurvatureCoefficient	0.2;
}

Dictionary	optimization
{	
	@Temperature			false;
	@Velocity				false;
	@Species				NO;
	@TemperatureProfile		T-Profile;
	@VelocityProfile		U-Profile;
	@SpeciesProfile			NO-Profile;
}

